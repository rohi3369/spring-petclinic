pipeline {
    agent none 
    stages {
        stage('learning') {
            steps {
                git url: 'https://github.com/rohi3369/spring-petclinic.git'     
            }
        }
           stage('Test') {
            agent { 'openjdk:11-jdk' } 
            steps {
                echo 'Hello, JDK'
                sh 'java -version'
            }
        }
    }
}