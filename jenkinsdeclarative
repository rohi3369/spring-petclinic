pipeline {
    agent none 
    stages {
        stage('learning') {
            steps {
                git url: 'https://github.com/rohi3369/spring-petclinic.git'
            }
        }
        stage('Example Test') {
            agent { 'openjdk:8-jre' } 
            steps {
                echo 'Hello, JDK'
                sh 'java -version'
            }
        }
    }
}