pipeline {
    agent none 
    stages {
        stage('learning') {
            steps {
                git url: 'https://github.com/rohi3369/spring-petclinic.git'  
                branch : 'branch1sp'   
            }
        }
        agent { 'openjdk:11-jdk' } 
           stage('Test') {
            steps {
                echo 'Hello, JDK'
                sh 'java -version'
            }
        }
    }
}